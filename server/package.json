{
  "name": "pokedex",
  "version": "0.0.1",
  "main": "dist/server.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "tsc && node dist/src/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "build": "tsc",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --config=jest.config.ts",
    "docker:build": "docker build -t pokedex-api ."
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "helmet": "^5.0.2"
  }
}
